{
  "Net.MaxOpenRequests": 5,
  "Net.DialTimeoutSeconds": 30,
  "Net.ReadTimeoutSeconds": 30,
  "Net.WriteTimeoutSeconds": 30,
  "Net.KeepAlive": 0,
  "Metadata.Retry.Max": 3,
  "Metadata.Retry.BackoffMilliseconds": 250,
  "Metadata.RefreshFrequencyMinutes": 10,
  "Producer.MaxMessageBytes": 1000000,
  "Producer.TimeoutSeconds": 10,
  "Producer.Return.Successes": false,
  "Producer.Return.Errors": false,
  "Producer.Retry.Max": 3,
  "Producer.Retry.MaxProcessingTimeMilliseconds": 100,
  "Producer.Flush.Messages": 1,
  "Producer.Flush.MaxMessages": 100,
  "Producer.Flush.FrequencyMilliseconds": 10,
  "Consumer.Retry.BackoffSeconds": 2,
  "Consumer.MaxWaitTimeMilliseconds": 250,
  "Consumer.Return.Errors": false,
  "ConsumerGroup.Config.Offsets.ProcessingTimeoutSeconds": 10,
  "ConsumerGroup.Config.Offsets.CommitIntervalSeconds": 1,
  "ClientID": "shubao",
  "ChannelBufferSize": 256,
  "Brokers": "{{kafka.hosts | join(',')}}",
  "Zookeepers": "{{zookeeper.hosts | join(',')}}"
}